<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th></th>
      <th>曲名</th>
      <th>感想</th>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% musics.each do |music| %>
      <tr id="music_<%= music.id %>">
        <%= render 'musics/item', music: music %>
      </tr>
    <% end %>
  </tbody>
</table>
